<!DOCTYPE html>
<html>
<head>
  <title>FbgraphApplication</title>
  <%= stylesheet_link_tag :all %>
  <%= stylesheet_link_tag 'uploadify' %>
  <%#= javascript_include_tag :defaults %>  
  <%= javascript_include_tag "/javascripts/uploadify/jquery-1.4.4.min.js" %>
  <%= javascript_include_tag "/javascripts/uploadify/swfobject.js" %>
  <%= javascript_include_tag "/javascripts/uploadify/jquery.uploadify.v2.1.4.min.js" %>

  <%= csrf_meta_tag %>
</head>
<body>

<div id="page">
  <% if flash[:alert] %>
    <p class="flash-error"><%= flash[:alert] %></p>
  <% end %>
  <% if flash[:notice] %>
    <p class="flash-notice"><%= flash[:notice] %></p>
  <% end %>
  <%= yield %>
</div>
   

</body>
</html>
